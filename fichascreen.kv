                        size: self.size
                    Color:
                        rgba: 1, 1, 1, 1
                    Rectangle:
                        pos: self.x + 2, self.y + 2
                        size: self.width - 4, self.height - 4
                    Color:
                        rgba: 0, 0, 0, 1

                Label:
