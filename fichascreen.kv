<FichaScreen>
    canvas.before:
        Color:
            rgba: 0.8, 0.8, 0.8, 1  # Define a cor de fundo
        Rectangle:
            pos: self.pos
            size: self.size
    ScrollView:
        do_scroll_x: False

        GridLayout:
            cols: 1
            size_hint_y: None
            height: self.minimum_height
            spacing: dp(0.5)
            padding: dp(10)

            AnchorLayout:
                anchor_x: 'center'
                anchor_y: 'top'
                spacing: dp(300)
                size_hint_y: None
                height: dp(50)

                BoxLayout:
                    orientation: 'horizontal'
                    spacing: dp(300)
                    
                    Button:
                        text: 'Voltar'
                        background_color: 0, 0, 0, 0  # Configura o fundo para ser transparente
                        color: 0.3, 0.3, 0.3, 1  # Configura a cor do texto para preto
                        font_size: 15  # Ajusta o tamanho da fonte para o "X" maior
                        size_hint: None, None
                        size: self.texture_size
                        pos_hint: {'center_x': 0.4, 'center_y': 0.8}
                        canvas.before:
                            Color:
                                rgba: 0.3, 0.3, 0.3, 1  # Define a cor do sublinhado
                            Line:
                                points: self.x, self.y - 5, self.x + self.width, self.y - 5  # Desenha o sublinhado
                                width: 1  # Define a largura do sublinhado
                        on_release: app.goto_inicial()

                    Label:
                        text: 'Ficha Técnica'
                        color: 0.1, 0.1, 0.1, 1
                        font_size: 24
                        size_hint: None, None
                        size: self.texture_size
                        pos_hint: {'center_y': 0.5}


            BoxLayout:
